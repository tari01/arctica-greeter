#: ../src/user-list.vala:898
msgid "Log In"
msgstr "Logi sisse"

#: ../src/user-list.vala:899
#, c-format
msgid "Login as %s"
msgstr "Meldi kui %s"

#: ../src/user-list.vala:903
msgid "Retry"
msgstr "Proovi uuesti"

#: ../src/user-list.vala:904
#, c-format
msgid "Retry as %s"
