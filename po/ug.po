#: ../src/user-list.vala:898
msgid "Log In"
msgstr "كىر"

#: ../src/user-list.vala:899
#, c-format
msgid "Login as %s"
msgstr "%s سۈپىتىدە كىرىڭ"

#: ../src/user-list.vala:903
msgid "Retry"
msgstr "قايتا سىنا"

#: ../src/user-list.vala:904
#, c-format
