#: ../src/user-list.vala:898
msgid "Log In"
msgstr "登录"

#: ../src/user-list.vala:899
#, c-format
msgid "Login as %s"
msgstr "以 %s 登录"

#: ../src/user-list.vala:903
msgid "Retry"
msgstr "重试"

#: ../src/user-list.vala:904
#, c-format
msgid "Retry as %s"
msgstr "以 %s 重试"

#: ../src/user-list.vala:948
